{
  "name": "unccodestore",
  "version": "1.0.0",
  "description": "mern stack workflow yt",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && cd frontend && npm install --include=dev && npm run build"
  },
