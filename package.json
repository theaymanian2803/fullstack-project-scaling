    "url": "https://github.com/theaymanian2803/unccodestore/issues"
  },
  "homepage": "https://github.com/theaymanian2803/unccodestore#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.20.0",
    "multer": "^2.0.2",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
